<%@ page language="java" contentType="text/html; charset=utf-8"	pageEncoding="utf-8"%><%@ page language="java" import="org.app.pojo.Sainput"%><%@taglib prefix="c" uri="http://java.sun.com/jstl/core_rt"%><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%><%@ page language="java" import="org.app.pojo.Income"%><!DOCTYPE html><html lang="en"><head><!-- BEGIN META --><meta charset="utf-8"><meta name="viewport" content="width=device-width, initial-scale=1.0"><meta name="description" content=""><meta name="author" content="Olive Enterprise"><!-- END META --><!-- BEGIN SHORTCUT ICON --><link rel="shortcut icon" href="img/favicon.ico"><!-- END SHORTCUT ICON --><title>Form Validation</title><!-- BEGIN STYLESHEET--><link href="/css/bootstrap.min.css" rel="stylesheet"><!-- BOOTSTRAP CSS --><link href="/css/bootstrap-reset.css" rel="stylesheet"><!-- BOOTSTRAP CSS --><link href="/assets/font-awesome/css/font-awesome.css" rel="stylesheet"><!-- FONT AWESOME ICON STYLESHEET --><link href="/css/style.css" rel="stylesheet"><!-- THEME BASIC CSS --><link href="/css/style-responsive.css" rel="stylesheet"><!-- THEME BASIC RESPONSIVE  CSS --><!--[if lt IE 9]>      <script src="/js/html5shiv.js"></script>      <script src="/js/respond.min.js"></script>      <![endif]--><!-- END STYLESHEET--></head><body>	<section class="wrapper">		<!-- BEGIN ROW  -->		<div class="row">			<div class="col-md-12">				<section class="panel">					<header class="panel-heading">						<span class="label label-primary">汇总费录入</span> <span							class="tools pull-right"> <a href="javascript:;"							class="fa fa-chevron-down"></a> <a href="javascript:;"							class="fa fa-times"></a>						</span>					</header>					<div class="panel-body">						<div class="form">							<form name="form" class="cmxform form-horizontal tasi-form"								id="signupFormy" method="post" action="/summary/insert">								<div class="form-group ">									<input class=" form-control" id="" name="incomeId"										type="hidden" readonly value="${income.getId()}" /><label										for="firstname" class="control-label col-md-1">收付款项目</label>									<div class="col-md-2">										<select class=" form-control" id="" name="flag" type="text">											<option value="S">应收费用</option>											<option value="F">应付费用</option>										</select>									</div>									<label for="firstname" class="control-label col-md-1">收款单位</label>									<div class="col-md-8">										<input class=" form-control" id="" name="sendCorp"											type="text" required="">									</div>								</div>								<div class="form-group ">									<label for="firstname" class="control-label col-md-1">单价</label>									<div class="col-md-1">										<input class=" form-control" id="" name="price" type="text"											required="">									</div>									<label for="password" class="control-label col-md-1">数量</label>									<div class="col-md-1">										<input class="form-control " id="" name="quantity" type="text"											required="">									</div>									<label for="password" class="control-label col-md-1">合计款</label>									<div class="col-md-2">										<input class="form-control " id="" name="actReceivable"											type="text" required="">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">对账日期</label>									<div class="col-md-3">										<input class=" form-control" id="dzrq" name="checkDate"											type="date" required="">									</div>									<label for="username" class="control-label col-md-1">清账日期</label>									<div class="col-md-3">										<input class="form-control " id="qzrq" name="settleDate"											type="date" required="">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">清账备注</label>									<div class="col-md-11">										<textarea class="form-control " id="" name="settleCmt"											required=""></textarea>									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">发站</label>									<div class="col-md-2">										<input class=" form-control" id="fz" name="arriveSation"											readonly="true" type="text" value="${sainput.getStation()}">									</div>									<label for="username" class="control-label col-md-1">发货单位</label>									<div class="col-md-2">										<input class="form-control " id="fhdw" name="arriveCorp"											readonly="true" type="text" value="${sainput.getCorp()}">									</div>									<label for="lastname" class="control-label col-md-1">到站</label>									<div class="col-md-2">										<input class=" form-control" id="dz" name="dz" readonly="true"											type="text">									</div>									<label for="lastname" class="control-label col-md-1">到达公司</label>									<div class="col-md-2">										<input class=" form-control" id="ddgs" readonly="true"											name="ddgs" type="text">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">品名</label>									<div class="col-md-8">										<input class=" form-control" id="" name="goodName"											readonly="true" type="text" value="${sainput.getGoodName()}">									</div>									<label for="username" class="control-label col-md-1">发货日期</label>									<div class="col-md-2">										<input class="form-control " id="fhrq" name="saDate"											type="date" value="">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">进出箱场地</label>									<div class="col-md-3">										<input class=" form-control" id="jcxcd2" name="cntrPlace"											readonly="true" type="text" value="${sainput.getCntrPlace()}">									</div>									<label for="lastname" class="control-label col-md-1">箱型号</label>									<div class="col-md-1">										<input class=" form-control" id="xxh2" readonly=""											readonly="true" name="cntrType"											value="${sainput.getCntrType()}" type="text">									</div>									<label for="lastname" class="control-label col-md-1">箱主代码</label>									<div class="col-md-2">										<input class=" form-control" id="xzdm" readonly="true"											name="cntrOwner" type="text"											value="${sainput.getCntrOwner()}">									</div>									<label for="lastname" class="control-label col-md-1">箱号</label>									<div class="col-md-2">										<input class=" form-control" id="xxh2" readonly=""											readonly="true" name="cntrNo" value="${sainput.getCntrNo()}"											type="text">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">票号</label>									<div class="col-md-2">										<input class=" form-control" id="xxh2" readonly=""											readonly="true" name="billNo" value="${sainput.getBillNo()}"											type="text">									</div>									<label for="username" class="control-label col-md-1">票货类型</label>									<div class="col-md-1">										<input class=" form-control" id="xxh2" readonly=""											readonly="true" name="billType"											value="${sainput.getBillType()}" type="text">									</div>									<label for="lastname" class="control-label col-md-1">封号</label>									<div class="col-md-1">										<input class=" form-control" id="xxh2" readonly=""											readonly="true" name="lockNo" value="${sainput.getLockNo()}"											type="text">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">收货人</label>									<div class="col-md-1">										<input class=" form-control" id="lastname" name="consignee"											type="text" readonly="true" value="${income.getConsignee() }">									</div>									<label for="lastname" class="control-label col-md-1">电话</label>									<div class="col-md-2">										<input class=" form-control" id="lastname" name="phone"											type="text" readonly="true" value="${income.getCorpPhone() }">									</div>									<label for="lastname" class="control-label col-md-1">送货地址</label>									<div class="col-md-5">										<input class=" form-control" id="lastname" name="shipaddress"											type="text" readonly="true"											value="${income.getCorpAddress() }">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">派送单号</label>									<div class="col-md-2">										<input class=" form-control" id="lastname" name="strcukNo"											type="text" readonly="true" value="${income.getStrcukNo() }">									</div>									<label for="lastname" class="control-label col-md-1">派送日期</label>									<div class="col-md-2">										<input class=" form-control" id="lastname" name="sendDate"											type="date" readonly="true"											value="<fmt:formatDate type='date' pattern='yyyy-MM-dd' value='${income.getSendDate()}'/>">									</div>									<label for="lastname" class="control-label col-md-1">驾驶员</label>									<div class="col-md-2">										<input class=" form-control" id="lastname" name="truckDriver"											type="text" readonly="true"											value="${income.getTruckDriver() }">									</div>									<label for="lastname" class="control-label col-md-1">集卡号</label>									<div class="col-md-2">										<input class=" form-control" id="lastname" name="truckMo"											type="text" readonly="true" value="${income.getTruckNo() }">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">备注</label>									<div class="col-md-11">										<textarea class="form-control " id="comment" name="comment"											required=""></textarea>									</div>								</div>								<div class="form-group">									<div class="col-md-offset-2 col-md-10">										<button class="btn btn-primary" type="submit">录入</button>                                        <button class="btn btn-default" type="reset">重置</button>									</div>								</div>							</form>						</div>					</div>				</section>			</div>		</div>		<!-- END ROW  -->	</section>	<!-- END WRAPPER  -->	<script src="/js/jquery.js"></script>	<!-- BASIC JS LIABRARY -->	<script src="/js/bootstrap.min.js"></script>	<!-- BOOTSTRAP JS  -->	<script src="/js/jquery.dcjqaccordion.2.7.js"></script>	<!-- ACCORDING JS -->	<script src="/js/jquery.scrollTo.min.js"></script>	<!-- SCROLLTO JS  -->	<script src="/js/jquery.nicescroll.js">			</script>	<!-- NICESCROLL JS  -->	<script src="/js/respond.min.js"></script>	<!-- RESPOND JS  -->	<script src="/js/jquery.validate.min.js"></script>	<!-- VALIDATE JS  -->	<script src="/js/common-scripts.js"></script>	<!-- BASIC COMMON JS  -->	<script src="/js/form-validation-script.js"></script>	<!-- FORM VALIDATION SCRIPT JS  -->	<!-- END JS -->	<script language="JavaScript">		today = new Date();        date = '';        month = '';        if (today.getMonth() + 1 < 10) {            month = '0' + (today.getMonth() + 1);        }else month = (today.getMonth() + 1);        if (today.getDate() < 10) {            date = '0' + today.getDate();        }else date = today.getDate();		form.dzrq.value = today.getFullYear() + '-' + month + '-' + date;		form.qzrq.value = today.getFullYear() + '-' + month + '-' + date;		form.fhrq.value = today.getFullYear() + '-' + month + '-' + date;	</script></body></html>