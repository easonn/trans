<%@ page contentType="text/html;charset=UTF-8" language="java"%><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%><!DOCTYPE html><html lang="en"><head><!-- BEGIN META --><meta charset="utf-8"><meta name="viewport" content="width=device-width, initial-scale=1.0"><meta name="description" content=""><meta name="author" content="Olive Enterprise"><!-- END META --><!-- BEGIN SHORTCUT ICON --><link rel="shortcut icon" href="/img/favicon.ico"><!-- END SHORTCUT ICON --><title>成都源通物流</title><!-- BEGIN STYLESHEET--><link href="/css/bootstrap.min.css" rel="stylesheet"><!-- BOOTSTRAP CSS --><link href="/css/bootstrap-reset.css" rel="stylesheet"><!-- BOOTSTRAP CSS --><link href="/assets/font-awesome//css/font-awesome.css" rel="stylesheet"><!-- FONT AWESOME ICON STYLESHEET --><link href="/css/style.css" rel="stylesheet"><!-- THEME BASIC CSS --><link href="/css/style-responsive.css" rel="stylesheet"><!-- THEME BASIC RESPONSIVE  CSS --><!--[if lt IE 9]>      <script src="/js/html5shiv.js"></script>      <script src="/js/respond.min.js"></script>      <![endif]--><!-- END STYLESHEET--></head><body>    <section class="wrapper">        <!-- BEGIN ROW  -->        <div class="row">            <div class="col-md-12">                <section class="panel">                    <header class="panel-heading">                        <span class="label label-primary">发票付款修改</span> <span                            class="tools pull-right"> <a href="javascript:;"                            class="fa fa-chevron-down"></a> <a href="javascript:;"                            class="fa fa-times"></a>                        </span>                    </header>                    <div class="panel-body">                        <div class="form">                            <form name="form" class="cmxform form-horizontal tasi-form"                                id="signupFormy" method="post"                                action="/bill/edit?id=${bill.getId()}">                                <div class="form-group ">                                    <label for="lastname" class="control-label col-md-1">收付款</label>                                    <div class="col-md-2">                                        <select class=" form-control" id="flag" name="flag"                                            type="text">                                            <option value="F" readyonly>付款</option>                                        </select>                                    </div>                                    <label for="lastname" id="xxh" class="control-label col-md-1">对账日期</label>                                    <div class="col-md-3">                                        <input class=" form-control" id="dzrq" name="checkDate"                                            value="<fmt:formatDate type='date' pattern='yyyy-MM-dd' value='${bill.getCheckDate() }'/>"                                            type="date">                                    </div>                                    <label for="username" class="control-label col-md-1">清账日期</label>                                    <div class="col-md-3">                                        <input class="form-control" id="qzrq" name="settleDate"                                            value="<fmt:formatDate type='date' pattern='yyyy-MM-dd' value='${bill.getSettleDate() }'/>"                                            type="date">                                    </div>                                </div>                                <div class="form-group ">                                    <label id="sfkbz" for="username" class="control-label col-md-1">收款备注</label>                                    <div class="col-md-11">                                        <textarea class="form-control " id="ccomment"                                            name="repComment" required="">${bill.getRepComment()}</textarea>                                    </div>                                    <div class="form-group "></div>                                    <label for="firstname" class="control-label col-md-1">开票日期</label>                                    <div class="col-md-2">                                        <input class=" form-control" id="kprq" name="billDate"                                            value="<fmt:formatDate type='date' pattern='yyyy-MM-dd' value='${bill.getBillDate()}'/>"                                            type="date" required="">                                    </div>                                    <label for="firstname" class="control-label col-md-1">交票日期</label>                                    <div class="col-md-2">                                        <input class=" form-control" id="jprq" name="billSendDate"                                            value="<fmt:formatDate type='date' pattern='yyyy-MM-dd' value='${bill.getBillSendDate()}' />"                                            type="date" date="">                                    </div>                                    <label for="firstname" class="control-label col-md-1">开票单位</label>                                    <div class="col-md-2">                                        <input class=" form-control" id="kpdw" name="billCorp"                                            value="${bill.getBillCorp()}" type="text" required="">                                    </div>                                    <label for="password" class="control-label col-md-1">票号</label>                                    <div class="col-md-2">                                        <input class="form-control " id="text"                                            onKeyUp="value=value.replace(/[\W]/g,'')" name="billNo"                                            value="${bill.getBillNo()}" type="text" required="">                                    </div>                                </div>                                <div class="form-group ">                                    <label for="lastname" class="control-label col-md-1">抵扣单位</label>                                    <div class="col-md-4">                                        <input class=" form-control" id="lastname"                                            name="deductionsCorp" type="text" required=""                                            value="${bill.getDeductionsCorp()}">                                    </div>                                    <label for="username" class="control-label col-md-1">款票面金额</label>                                    <div class="col-md-1">                                        <input class="form-control " id="" name="price"                                            onkeyup="clearNoNum(this)" type="text" required=""                                            value="${bill.getPrice()}">                                    </div>                                    <label id="sffpd" for="username" class="control-label col-md-1">收发票点</label>                                    <div class="col-md-1">                                        <input class="form-control " id="username" name="billPoint"                                            onkeyup="value=value.replace(/[^\d.]/g,'')" type="text"                                            required="" value="${bill.getBillPoint()}">                                    </div>                                    <label id="sfpk" for="lastname" class="control-label col-md-1">收票款</label>                                    <div class="col-md-2">                                        <input class=" form-control" id="lastname" name="billPrice"                                            onkeyup="value=value.replace(/[^\d.]/g,'')" type="text"                                            required="" value="${bill.getBillPrice()}">                                    </div>                                </div>                                <div class="form-group">                                    <div class="col-md-offset-2 col-md-6">                                        <button class="btn btn-primary" type="submit">更新</button>                                        <a href="/bill/query?flag=F"><button class="btn btn-default" type="button">取消</button></a>                                    </div>                                    <label class="control-label col-md-6"><font                                        style="text-align: center" color="red">${info}</font></label>                                </div>                            </form>                        </div>                    </div>                </section>            </div>        </div>        <!-- END ROW  -->    </section>    <!-- END WRAPPER  -->    <script src="/js/jquery.js"></script>    <!-- BASIC JS LIABRARY -->    <script src="/js/bootstrap.min.js"></script>    <!-- BOOTSTRAP JS  -->    <script src="/js/jquery.dcjqaccordion.2.7.js"></script>    <!-- ACCORDING JS -->    <script src="/js/jquery.scrollTo.min.js"></script>    <!-- SCROLLTO JS  -->    <script src="/js/jquery.nicescroll.js">            </script>    <!-- NICESCROLL JS  -->    <script src="/js/respond.min.js"></script>    <!-- RESPOND JS  -->    <script src="/js/jquery.validate.min.js"></script>    <!-- VALIDATE JS  -->    <script src="/js/common-scripts.js"></script>    <!-- BASIC COMMON JS  -->    <script src="/js/form-validation-script.js"></script>    <script language="JavaScript">        function clearNoNum(obj) {            obj.value = obj.value.replace(/[^\d.]/g, ""); //清除“数字”和“.”以外的字符              obj.value = obj.value.replace(/\.{2,}/g, "."); //只保留第一个. 清除多余的              obj.value = obj.value.replace(".", "$#$").replace(/\./g, "")                    .replace("$#$", ".");            obj.value = obj.value.replace(/^(\-)*(\d+)\.(\d\d).*$/, '$1$2.$3');//只能输入两个小数              if (obj.value.indexOf(".") < 0 && obj.value != "") {//以上已经过滤，此处控制的是如果没有小数点，首位不能为类似于 01、02的金额                 obj.value = parseFloat(obj.value);            }        }    </script>    <!-- FORM VALIDATION SCRIPT JS  -->    <!-- END JS --></body></html>