<%@ page language="java" contentType="text/html; charset=utf-8"	pageEncoding="utf-8"%><%@ page language="java" import="org.app.pojo.Sainput"%><%@taglib prefix="c" uri="http://java.sun.com/jstl/core_rt"%><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%><!DOCTYPE html><html lang="en"><head><!-- BEGIN META --><meta charset="utf-8"><!-- END META --><!-- BEGIN SHORTCUT ICON --><link rel="shortcut icon" href="img/favicon.ico"><!-- END SHORTCUT ICON --><title>成都源通物流</title><!-- BEGIN STYLESHEET --><link href="/css/bootstrap.min.css" rel="stylesheet"><!-- BOOTSTRAP CSS --><link href="/css/bootstrap-reset.css" rel="stylesheet"><!-- BOOTSTRAP CSS --><link href="/assets/font-awesome/css/font-awesome.css" rel="stylesheet"><!-- FONT AWESOME ICON STYLESHEET --><link rel="stylesheet" href="/assets/data-tables/DT_bootstrap.css"><!-- DATATABLE CSS --><link href="/css/style.css" rel="stylesheet"><!-- THEME BASIC CSS --><link href="/css/style-responsive.css" rel="stylesheet"><!-- THEME BASIC RESPONSIVE  CSS --><!--[if lt IE 9]>      <script src="/js/html5shiv.js"></script>      <script src="/js/respond.min.js"></script>      <![endif]--><!-- END STYLESHEET --></head><body>	<section class="wrapper">		<!-- BEGIN ROW  -->		<div class="row">			<div class="col-md-12">				<section class="panel">					<header class="panel-heading">						<span class="label label-primary">发货编辑</span> <span							class="tools pull-right"> <a href="javascript:;"							class="fa fa-chevron-down"></a> <a href="javascript:;"							class="fa fa-times"></a>						</span>					</header>					<div class="panel-body">						<div class="form">							<form class="cmxform form-horizontal tasi-form" id="signupFormy"								method="post" action="/sainput/edit?id=${sainput.getId()}">								<div class="form-group ">									<label for="firstname" class="control-label col-md-1">到发类型</label>									<div class="col-md-1">										<select class=" form-control" id="flag" name="flag"											type="text" readonly>											<option value="S">发货</option>										</select>									</div>									<label for="firstname" class="control-label col-md-1">箱型号</label>									<div class="col-md-1">										<select onChange="dd()" class=" form-control" id=""											name="cntrType" type="text" readonly>											<option value="${sainput.getCntrType()}">${sainput.getCntrType()}</option>										</select>									</div>									<label for="lastname" class="control-label col-md-1">箱主代码</label>									<div class="col-md-3">										<input class=" form-control" id="" name="cntrOwner"											type="text" required="" value="${sainput.getCntrOwner()}">									</div>									<label for="username" class="control-label col-md-1">箱号</label>									<div class="col-md-3">										<input class="form-control " id="" name="cntrNo" type="text"											required="" value="${sainput.getCntrNo()}">									</div>								</div>								<div class="form-group ">									<label id="fhsj" for="lastname" class="control-label col-md-1">发货时间</label>									<div class="col-md-2">										<input class=" form-control" name="date" type="date"											required="" pattern='yyyy-MM-dd'											value="<fmt:formatDate type='date' pattern='yyyy-MM-dd' value='${sainput.getDate()}'/>">									</div>									<label for="lastname" class="control-label col-md-1">进出箱场地</label>									<div class="col-md-2">										<input class=" form-control" id="" name="cntrPlace"											type="text" required="" value="${sainput.getCntrPlace()}">									</div>									<label id="station" for="lastname"										class="control-label col-md-1">发站</label>									<div class="col-md-2">										<input class=" form-control" id=c "station" name="station"											type="text" required="" value="${sainput.getStation()}">									</div>									<label id="crop" for="lastname" class="control-label col-md-1">发货单位</label>									<div class="col-md-2">										<input class=" form-control" id="corp" name="corp" type="text"											required="" value="${sainput.getCorp()}">									</div>								</div>								<div class="form-group ">									<label for="firstname" class="control-label col-md-1">票号</label>									<div class="col-md-2">										<input class=" form-control" id="" name="billNo" type="text"											required="" value="${sainput.getBillNo()}">									</div>									<label for="password" class="control-label col-md-1">封号</label>									<div class="col-md-1">										<input class="form-control " id="" name="lockNo" type="text"											required="" value="${sainput.getLockNo()}">									</div>									<label for="username" class="control-label col-md-1">品名</label>									<div class="col-md-4">										<input class="form-control " id="" name="goodName" type="text"											required="" value="${sainput.getGoodName()}">									</div>									<label for="username" class="control-label col-md-1">货票类型</label>									<div class="col-md-1">										<input class="form-control " required="" id="" name="billType"											type="text" value="${sainput.getBillType()}">									</div>								</div>								<div class="form-group">									<div class="col-md-offset-2 col-md-10">										<button class="btn btn-primary" type="submit">更新</button>										<a href="/sainput/query?flag=S"><button												class="btn btn-default" type="button">取消</button></a>									</div>								</div>							</form>						</div>					</div>				</section>			</div>		</div>		<!-- END ROW  -->		</div>	</section>	<!-- BEGIN JS -->	<script src="/js/jquery-1.8.3.min.js"></script>	<!-- BASIC JS LIABRARY 1.8.3 -->	<script src="/js/bootstrap.min.js"></script>	<!-- BOOTSTRAP JS  -->	<script src="/js/jquery.dcjqaccordion.2.7.js"></script>	<!-- ACCORDING JS -->	<script src="/js/jquery.scrollTo.min.js"></script>	<!-- SCROLLTO JS  -->	<script src="/js/jquery.nicescroll.js"></script>	<!-- NICESCROLL JS  -->	<script src="/js/respond.min.js"></script>	<!-- RESPOND JS  -->	<script src="/js/jquery.validate.min.js"></script>	<!-- VALIDATE JS  -->	<script src="/js/common-scripts.js"></script>	<!-- BASIC COMMON JS  -->	<script src="/js/form-validation-script.js"></script>	</script>	<!-- NICESCROLL JS  -->	<script src="/assets/data-tables/jquery.dataTables.js"></script>	<!-- DATATABLE JS  -->	<script src="/assets/data-tables/DT_bootstrap.js"></script>	<!-- DATATABLE JS  -->	<script src="/js/respond.min.js"></script>	<!-- RESPOND JS  -->	<script src="/js/common-scripts.js"></script>	<!-- BASIC COMMON JS  -->	<script src="/js/editable-table.js"></script>	<!-- EDITABLE TABLE JS  -->	<script>	<!-- EDITABLE TABLE FUNCTION  -->		jQuery(document).ready(function() {			EditableTable.init();		});	</script>	<!-- FORM VALIDATION SCRIPT JS  -->	<!-- END JS --></body></html>