trol-label col-md-1">到发类型</label>									<div class="col-md-1">							<select class=" form-control" id="flag" name="flag"											type="text" readonly>											<c:choose>												<c:when test="${income.getFlag().equals('S')}">													<option value="S">发货</option>												</c:when>												<c:otherwise>													<option value="F">到货</option>												</c:otherwise>											</c:choose>										</select>										   <label                                    for="firstname" class="control-label col-md-1">进出箱场地</label>                                <div class="col-md-2">                                    <input class=" form-control" id="" name="cntrPlace"                                        type="hidden" readonly                                        value="${income.getSainput().getCntrPlace()}">                                </div>                                <label                                    for="firstname" class="control-label col-md-1">到达公司</label>                                <div class="col-md-2">                                    <input class=" form-control" id="" name="acropName" type="text"                                        value="${income.getSainput().getCorp()}" readonly>                                </div>                                                                     <label                                    for="password" class="control-label col-md-1">箱号</label>                                                                    <div class="col-md-3">                                    <input class="form-control " id="" name="cntrNo" type="text"                                        value="${income.getSainput().getCntrNo()}" readonly>                                </div>									</div> -->								<input type="hidden" name="flag" value="${income.getFlag()}">								<input name="cntrPlace" type="hidden"									value="${income.getSainput().getCntrPlace()}"> <input									name="acropName" type="hidden"									value="${income.getSainput().getCorp()}"> <input									name="cntrNo" type="hidden"									value="${income.getSainput().getCntrNo()}">								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">收货人</label>									<div class="col-md-1">										<input class=" form-control" id="" name="consignee"											type="text" required="" value="${income.getConsignee()}">									</div>									<label for="username" class="control-label col-md-1">送货地址</label>									<div class="col-md-3">										<input class="form-control " id="" name="corpAddress"											type="text" required="" value="${income.getCorpAddress()}">									</div>									<label for="lastname" class="control-label col-md-1">电话</label>									<div class="col-md-2">										<input class=" form-control" id="" name="corpPhone"											type="text" required="" value="${income.getCorpPhone()}">									</div>									<label for="username" class="control-label col-md-1">付款人</label>									<div class="col-md-2">										<input class="form-control " id="" name="corpPayer"											type="text" required="" value="${income.getCorpPayer()}">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">站到门费用</label>									<div class="col-md-2">										<input class=" form-control" id="" name="stgCost" type="text"											required="" value="${income.getStgCost()}">									</div>									<label for="username" class="control-label col-md-1">站到站费</label>									<div class="col-md-2">										<input class="form-control " id="" name="stsCost" type="text"											required="" value="${income.getStsCost()}">									</div>									<label for="lastname" class="control-label col-md-1">代收铁运费</label>									<div class="col-md-2">										<input class=" form-control" id="" name="transExp" type="text"											required="" value="${income.getTransExp()}">									</div>									<label for="username" class="control-label col-md-1">超期杂费</label>									<div class="col-md-2">										<input class="form-control " id="" name="incidentalExp"											type="text" required="" value="${income.getIncidentalExp()}">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">派车单号</label>									<div class="col-md-2">										<input class=" form-control" id="" name="strcukNo" type="text"											required="" value="${income.getStrcukNo()}">									</div>									<label for="lastname" class="control-label col-md-1">派送日期</label>									<div class="col-md-2">										<input class=" form-control" id="psrq" name="sendDate" type="date"											required="" pattern='yyyy-MM-dd'											value="<fmt:formatDate type='date' pattern='yyyy-MM-dd' value='${income.getSendDate()}'/>">									</div>									<label for="username" class="control-label col-md-1">驾驶员</label>									<div class="col-md-2">										<input class="form-control " id="username" name="truckDriver"											type="text" required="" value="${income.getTruckDriver()}">									</div>									<label for="username" class="control-label col-md-1">集卡号</label>									<div class="col-md-2">										<input class="form-control " id="username" name="truckNo"											type="text" required="" value="${income.getTruckNo()}">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">备注</label>									<div class="col-md-11">										<textarea class="form-control " id="ccomment" name="comment">${income.getComment()}</textarea>									</div>								</div>								<div class="form-group">									<div class="col-md-offset-2 col-md-10">										<button class="btn btn-primary" type="submit">更新</button>										<a href="/income/query?flag=S"><button												class="btn btn-default" type="button">取消</button></a>									</div>								</div>							</form>						</div>					</div>				</section>			</div>		</div>		<!-- END ROW  -->	</section>	<!-- END WRAPPER  -->	<script src="/js/jquery.js"></script>	<!-- BASIC JS LIABRARY -->	<script src="/js/bootstrap.min.js"></script>	<!-- BOOTSTRAP JS  -->	<script src="/js/jquery.dcjqaccordion.2.7.js"></script>	<!-- ACCORDING JS -->	<script src="/js/jquery.scrollTo.min.js"></script>	<!-- SCROLLTO JS  -->	<script src="/js/jquery.nicescroll.js">			</script>	<!-- NICESCROLL JS  -->	<script src="/js/respond.min.js"></script>	<!-- RESPOND JS  -->	<script src="/js/jquery.validate.min.js"></script>	<!-- VALIDATE JS  -->	<script src="/js/common-scripts.js"></script>	<!-- BASIC COMMON JS  -->	<script src="/js/form-validation-script.js"></script>	<!-- FORM VALIDATION SCRIPT JS  -->	<!-- END JS -->	<script language="JavaScript">		today = new Date();		if (today.getMonth() + 1 < 10) {			month = '0' + (today.getMonth() + 1);		}		if (today.getDate() < 10) {			date = '0' + today.getDate();		}		form.psrq.value = today.getFullYear() + '-' + month + '-' + date;	</script></body></html>