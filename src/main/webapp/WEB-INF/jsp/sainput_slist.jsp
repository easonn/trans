<%@ page language="java" contentType="text/html; charset=utf-8"	pageEncoding="utf-8"%><%@ page language="java" import="org.app.pojo.Sainput"%><%@taglib prefix="c" uri="http://java.sun.com/jstl/core_rt"%><!DOCTYPE html><html lang="en"><head><!-- BEGIN META --><meta charset="utf-8"><!-- END META --><!-- BEGIN SHORTCUT ICON --><link rel="shortcut icon" href="img/favicon.ico"><!-- END SHORTCUT ICON --><title>成都源通物流</title><!-- BEGIN STYLESHEET --><link href="/css/bootstrap.min.css" rel="stylesheet"><!-- BOOTSTRAP CSS --><link href="/css/bootstrap-reset.css" rel="stylesheet"><!-- BOOTSTRAP CSS --><link href="/assets/font-awesome//css/font-awesome.css" rel="stylesheet"><!-- FONT AWESOME ICON STYLESHEET --><link rel="stylesheet" href="/assets/data-tables/DT_bootstrap.css"><!-- DATATABLE CSS --><link href="/css/style.css" rel="stylesheet"><!-- THEME BASIC CSS --><link href="/css/style-responsive.css" rel="stylesheet"><!-- THEME BASIC RESPONSIVE  CSS --><!--[if lt IE 9]>      <script src="/js/html5shiv.js"></script>      <script src="/js/respond.min.js"></script>      <![endif]--><!-- END STYLESHEET --></head><body>	<section class="wrapper site-min-height">		<!-- BEGIN ROW  -->		<div class="row">			<div class="col-md-12">								<section class="panel">					<header class="panel-heading">						<span class="label label-primary">发货清单/派车</span>					</header>					<div class="panel-body">						<div class="adv-table editable-table ">							<div class="space15"></div>							<table sytle="width:100%"								class="table table-striped table-hover table-bordered"								id="editable-sample">								<thead>									<tr>										<th>箱型</th>										<th>发站</th>										<th>箱主代码</th>										<th>进出箱场地</th>										<th>箱号</th>										<th>票货类型</th>										<th>品名</th>										<th>封号</th>										<th>发货单位</th>										<th>派车状态</th>										<th>编辑</th>										<th>删除</th>									</tr>								</thead>								<tbody>									<c:forEach items="${sainputsList}" var="sainput"										varStatus="status">										<c:if test="${(status.count%2)==0}">											<tr class="odd">										</c:if>										<td><c:out value="${sainput.getCntrType()}"></c:out></td>										<td><c:out value="${sainput.getStation()}" /></td>										<td><c:out value="${sainput.getCntrOwner()}" /></td>										<td><c:out value="${sainput.getCntrPlace()}"></c:out></td>										<td><c:out value="${sainput.getCntrNo()}"></c:out></td>										<td><c:out value="${sainput.getBillType()}"></c:out></td>										<td><c:out value="${sainput.getGoodName()}"></c:out></td>										<td><c:out value="${sainput.getLockNo()}"></c:out></td>										<td><c:out value="${sainput.getCorp()}"></c:out></td>										<td><c:choose>												<c:when test="${sainput.getIncomeState().equals('N')}">													<a class="" href="/income/insert?id=${sainput.getId() }"><span														class="label label-danger">未派车</span></a>												</c:when>												<c:otherwise><!-- 													<a readonly class="" href="/income/edit?id=${sainput.getId() }"><span														class="label label-success">已派车</span></a> --><span                                                        class="label label-success">已派车</span>												</c:otherwise>											</c:choose></td>										<td><a class=""											href="/sainput/edit?id=${sainput.getId() }"><span												class="label label-success">编辑</span></a></td>										<td><a class="delete"											href="/sainput/delete?id=${sainput.getId() }&flag=F"><span												class="label label-danger">删除</span></a></td>										</tr>									</c:forEach>								</tbody>							</table>						</div>					</div>				</section>			</div>		</div>		<!-- END ROW  -->		</div>	</section>	<!-- BEGIN JS -->	<script src="/js/jquery-1.8.3.min.js"></script>	<!-- BASIC JS LIABRARY 1.8.3 -->	<script src="/js/bootstrap.min.js"></script>	<!-- BOOTSTRAP JS  -->	<script src="/js/jquery.dcjqaccordion.2.7.js"></script>	<!-- ACCORDING JS -->	<script src="/js/jquery.scrollTo.min.js"></script>	<!-- SCROLLTO JS  -->	<script src="/js/jquery.nicescroll.js"></script>	<!-- NICESCROLL JS  -->	<script src="/js/respond.min.js"></script>	<!-- RESPOND JS  -->	<script src="/js/jquery.validate.min.js"></script>	<!-- VALIDATE JS  -->	<script src="/js/common-scripts.js"></script>	<!-- BASIC COMMON JS  -->	<script src="/js/form-validation-script.js"></script>	</script>	<!-- NICESCROLL JS  -->	<script src="/assets/data-tables/jquery.dataTables.js"></script>	<!-- DATATABLE JS  -->	<script src="/assets/data-tables/DT_bootstrap.js"></script>	<!-- DATATABLE JS  -->	<script src="/js/respond.min.js"></script>	<!-- RESPOND JS  -->	<script src="/js/common-scripts.js"></script>	<!-- BASIC COMMON JS  -->	<script src="/js/editable-table.js"></script>	<!-- EDITABLE TABLE JS  -->	<script>	<!-- EDITABLE TABLE FUNCTION  -->		jQuery(document).ready(function() {			EditableTable.init();		});	</script>	<script language="JavaScript">		today = new Date();		if (today.getMonth() + 1 < 10) {			month = '0' + (today.getMonth() + 1);		}		if (today.getDate() < 10) {			date = '0' + today.getDate();		}		form.fhrq.value = today.getFullYear() + '-' + month + '-' + date;		$('#flag').on('change', function() {			if (this.value == "F") {				document.getElementById("station").innerHTML = "到站";				document.getElementById("crop").innerHTML = "到达公司";				document.getElementById("fhsj").innerHTML = "到达日期";			} else if (this.value == "S") {				document.getElementById("station").innerHTML = "发站";				document.getElementById("crop").innerHTML = "发货单位";				document.getElementById("fhsj").innerHTML = "发货日期";			}		});	</script>	<!-- FORM VALIDATION SCRIPT JS  -->	<!-- END JS --></body></html>