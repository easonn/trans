<%@ page language="java" contentType="text/html; charset=utf-8"	pageEncoding="utf-8"%><!DOCTYPE html><html lang="en"><head><!-- BEGIN META --><meta charset="utf-8"><meta name="viewport" content="width=device-width, initial-scale=1.0"><meta name="description" content=""><meta name="author" content="Olive Enterprise"><!-- END META --><!-- BEGIN SHORTCUT ICON --><link rel="shortcut icon" href="img/favicon.ico"><!-- END SHORTCUT ICON --><title>Form Validation</title><!-- BEGIN STYLESHEET--><link href="/css/bootstrap.min.css" rel="stylesheet"><!-- BOOTSTRAP CSS --><link href="/css/bootstrap-reset.css" rel="stylesheet"><!-- BOOTSTRAP CSS --><link href="/assets/font-awesome/css/font-awesome.css" rel="stylesheet"><!-- FONT AWESOME ICON STYLESHEET --><link href="/css/style.css" rel="stylesheet"><!-- THEME BASIC CSS --><link href="/css/style-responsive.css" rel="stylesheet"><!-- THEME BASIC RESPONSIVE  CSS --><!--[if lt IE 9]>      <script src="/js/html5shiv.js"></script>      <script src="/js/respond.min.js"></script>      <![endif]--><!-- END STYLESHEET--></head><body>	<section class="wrapper">		<!-- BEGIN ROW  -->		<div class="row">			<div class="col-md-12">				<section class="panel">					<header class="panel-heading">						<span class="label label-primary">整车录入</span> <span							class="tools pull-right"> <a href="javascript:;"							class="fa fa-chevron-down"></a> <a href="javascript:;"							class="fa fa-times"></a>						</span>					</header>					<div class="panel-body">						<div class="form">							<form name="form" class="cmxform form-horizontal tasi-form"								id="signupFormy" method="post" action="/carload/insert">								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">收付款</label>									<div class="col-md-2">										<select class=" form-control" id="flag" name="flag"											type="text">											<option value="S">收款</option>											<option value="F">付款</option>										</select>									</div>									<label for="lastname" id="xxh" class="control-label col-md-1">对账日期</label>									<div class="col-md-3">										<input class=" form-control" id="dzrq" name="checkDate"											type="date" required="">									</div>									<label for="username" class="control-label col-md-1">清账日期</label>									<div class="col-md-3">										<input class="form-control " id="qzrq" name="settleDate"											type="date" required="">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">发站</label>									<div class="col-md-3">										<input class=" form-control" id="lastname" name="sendStation"											type="text" required="">									</div>									<label for="username" class="control-label col-md-1">到站</label>									<div class="col-md-3">										<input class="form-control " id="username"											name="arriveStation" type="text" required="">									</div>									<label for="lastname" class="control-label col-md-1">到达日期</label>									<div class="col-md-3">										<input class=" form-control" id="darq" name="arriveDate"											type="date" required="">									</div>								</div>								<div class="form-group ">									<label for="lastname" class="control-label col-md-1">票号</label>									<div class="col-md-2">										<input class=" form-control" id="lastname" name="billNo"											type="text" required="">									</div>									<label for="lastname" class="control-label col-md-1">品名</label>									<div class="col-md-2">										<input class=" form-control" id="lastname" name="good"											type="text" required="">									</div>									<label for="lastname" class="control-label col-md-1">数量</label>									<div class="col-md-1">										<input class=" form-control" id="lastname" name="quantity"											type="text" required="" onkeyup="clearNoNum(this)">									</div>									<label for="lastname" class="control-label col-md-1">重量</label>									<div class="col-md-1">										<input class=" form-control" id="lastname" name="weight"											type="text" required="" onkeyup="clearNoNum(this)">									</div>									<label for="lastname" class="control-label col-md-1">发票金额</label>									<div class="col-md-1">										<input class=" form-control" id="lastname" name="invoice"											type="text" required="" onkeyup="clearNoNum(this)">									</div>								</div>								<div class="form-group ">									<label id="sfkxm" for="lastname" class="control-label col-md-1">收款项目</label>									<div class="col-md-3">										<input class=" form-control" name="repaProjc" type="text"											required="">									</div>									<label id="sfkdj" for="lastname" class="control-label col-md-1">收款单价</label>									<div class="col-md-1">										<input class=" form-control" name="price" type="text"											required="" onkeyup="clearNoNum(this)">									</div>									<label id="ysfk" for="lastname" class="control-label col-md-1">应收款</label>									<div class="col-md-1">										<input class=" form-control" name="receivable" type="text"											required="" onkeyup="clearNoNum(this)">									</div>								</div>								<div class="form-group ">									<label id="sfkbz" for="username" class="control-label col-md-1">收款备注</label>									<div class="col-md-11">										<textarea class="form-control " id="ccomment" name="saComment"											required=""></textarea>									</div>								</div>								<div class="form-group">									<div class="col-md-offset-2 col-md-10">										<button class="btn btn-primary" type="submit">录入</button>                                        <button class="btn btn-default" type="reset">重置</button>									</div>								</div>							</form>						</div>					</div>				</section>			</div>		</div>		<!-- END ROW  -->	</section>	<!-- END WRAPPER  -->	<script src="/js/jquery.js"></script>	<!-- BASIC JS LIABRARY -->	<script src="/js/bootstrap.min.js"></script>	<!-- BOOTSTRAP JS  -->	<script src="/js/jquery.dcjqaccordion.2.7.js"></script>	<!-- ACCORDING JS -->	<script src="/js/jquery.scrollTo.min.js"></script>	<!-- SCROLLTO JS  -->	<script src="/js/jquery.nicescroll.js"></script>	</script>	<!-- NICESCROLL JS  -->	<script src="/js/respond.min.js"></script>	<!-- RESPOND JS  -->	<script src="/js/jquery.validate.min.js"></script>	<!-- VALIDATE JS  -->	<script src="/js/common-scripts.js"></script>	<!-- BASIC COMMON JS  -->	<script src="/js/form-validation-script.js"></script>	<script language="javaScript">		today = new Date();        date = '';        month = '';        if (today.getMonth() + 1 < 10) {            month = '0' + (today.getMonth() + 1);        }else month = (today.getMonth() + 1);        if (today.getDate() < 10) {            date = '0' + today.getDate();        }else date = today.getDate();		form.dzrq.value = today.getFullYear() + '-' + month + '-' + date;		form.qzrq.value = today.getFullYear() + '-' + month + '-' + date;		form.darq.value = today.getFullYear() + '-' + month + '-' + date;		$('#flag').on('change', function() {			if (this.value == "F") {				document.getElementById("sfkxm").innerHTML = "付款项目";				document.getElementById("sfkdj").innerHTML = "付款单价";				document.getElementById("ysfk").innerHTML = "应付款";				document.getElementById("sfkbz").innerHTML = "付款备注";			} else if (this.value == "S") {				document.getElementById("sfkxm").innerHTML = "收款项目";				document.getElementById("sfkdj").innerHTML = "收款单价";				document.getElementById("ysfk").innerHTML = "应收款";				document.getElementById("sfkbz").innerHTML = "收款备注";			}		})		function clearNoNum(obj) {			obj.value = obj.value.replace(/[^\d.]/g, ""); //清除“数字”和“.”以外的字符  			obj.value = obj.value.replace(/\.{2,}/g, "."); //只保留第一个. 清除多余的  			obj.value = obj.value.replace(".", "$#$").replace(/\./g, "")					.replace("$#$", ".");			obj.value = obj.value.replace(/^(\-)*(\d+)\.(\d\d).*$/, '$1$2.$3');//只能输入两个小数  			if (obj.value.indexOf(".") < 0 && obj.value != "") {//以上已经过滤，此处控制的是如果没有小数点，首位不能为类似于 01、02的金额 				obj.value = parseFloat(obj.value);			}		}	</script>	<!-- FORM VALIDATION SCRIPT JS  -->	<!-- END JS --></body></html>